#include <iostream>
using namespace std;
int rotate(int arr[], int size, int k)
{
    int temp[100],j=0;
    
  
    
    
        for(int i=k;i<size;i++)
        {
             temp[j]=arr[i];
             cout<<temp[j]<<" ";
             j++;
            
        }
        for(int i=0;i<k;i++)
        {
            cout<<arr[i]<<" ";
        }
     
     
   
  
   
}

int main() {
   int arr[] = {3,5,11,2,0,8};
   int size = sizeof(arr)/sizeof(int);
   int k, b[100];
   cout<<"Enter value of k ";
   cin>>k;
   rotate(arr,size,k);

    return 0;
}
